<script>
  const dialogOverlay = document.getElementById('dialogOverlay');
  const dialogCloseButton = document.querySelector('.dialog-close');
  
  // Close dialog when clicking overlay
  dialogOverlay.addEventListener('click', (e) => {
    const dialogGuideline = document.querySelector('.tradein-guideline-container');
    if (e.target === dialogOverlay && !!dialogGuideline) {
      closeDialog();
    }
  });
  
  // Close dialog with Escape key
  document.addEventListener('keydown', (e) => {
    const dialogGuideline = document.querySelector('.tradein-guideline-container');
    if (e.key === 'Escape' && dialogOverlay?.classList.contains('active') && !!dialogGuideline) {
      closeDialog();
    }
  });

  function closeDialog() {
    const tradeinChoice = document.querySelector('input[name="tradein"][value="yes"]');
    const htmlElement = document.querySelector('html');

    dialogOverlay.classList.remove('active');
    htmlElement.classList.remove('dialog-open');
    document.documentElement.style.removeProperty('--scrollbar-width');
    if(!!tradeinChoice) tradeinChoice.checked = false;
    dialogCloseButton.classList.remove('disable');
  }
</script>

                
              </div>


