    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const items = document.querySelectorAll('.valo-item');
    
            items.forEach(item => {
                const btn = item.querySelector('.valo-question');
                const answer = item.querySelector('.valo-answer');
                const icon = item.querySelector('.valo-icon');
    
                btn.addEventListener('click', () => {
                    const isActive = item.classList.contains('active');
    
                    items.forEach(otherItem => {
                        otherItem.classList.remove('active');
                        otherItem.querySelector('.valo-answer').style.maxHeight = null;
                        otherItem.querySelector('.valo-icon').innerText = '+';
                    });
                        if (!isActive) {
                        item.classList.add('active');
                        answer.style.maxHeight = answer.scrollHeight + "px";
                        icon.innerText = '-'; 
                    }
                });
            });
        });
    </script>
